{
	"name": "my-app",
	"version": "0.0.8",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"type-check": "tsc --noEmit",
		"lint": "eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && npm run type-check && npm run lint && npm run format && npm run test",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npm run test:unit:run && npm run test:playwright",
		"test:playwright": "echo \"playwright test skipped\" # playwright test",
		"test:unit": "vitest",
		"test:unit:run": "vitest run",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"data:wiki": "npm run data:wiki:downloadWiki && npm run data:wiki:parseWiki && npm run data:wiki:downloadSvgs && npm run data:wiki:applyParsedWiki",
		"data:wiki:downloadWiki": "vite-node ./src/data/wiki/downloadWiki/downloadWiki.js && prettier ./src/data/ --write",
		"data:wiki:parseWiki": "vite-node ./src/data/wiki/parseWiki/parseWiki.ts && prettier ./src/data/ --write",
		"data:wiki:downloadSvgs": "vite-node ./src/data/wiki/downloadSvgs/downloadSvgs.ts",
		"data:wiki:applyParsedWiki": "vite-node ./src/data/wiki/parseWiki/applyParsedWiki.ts && prettier ./src/data/ --write",
		"deploy": "chmod +x ./deploy.sh && ./deploy.sh",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"husky": "^8.0.3",
		"postcss": "^8.4.23",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-hero-icons": "^5.0.0",
		"sveltekit-search-params": "^1.0.12",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/overpass": "^4.5.11",
		"@fontsource/overpass-mono": "^4.5.11",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@total-typescript/ts-reset": "^0.4.2",
		"axios": "^1.4.0",
		"cheerio": "^1.0.0-rc.12",
		"clsx": "^1.2.1",
		"vite-node": "^0.30.1"
	}
}
